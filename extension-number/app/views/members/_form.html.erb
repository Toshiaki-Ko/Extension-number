<%= form_for(@member) do |f| %>
 
  <div class="field">
    <%= f.label :部署 %><br>
    <%= f.text_field :busho %>
  </div>
  <div class="field">
    <%= f.label :名前 %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :読み方 %><br>
    <%= f.text_field :yomikata %>
  </div>
  <div class="field">
    <%= f.label :番号 %><br>
    <%= f.number_field :number %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>
