= render "errors", obj: @member
%table.attr
  %tr
    - options = ["織田軍", "伊達軍", "徳川軍"]
    %th= form.label :department
    %td= form.select :department, options
  %tr
    %th= form.label :name
    %td= form.text_field :name, size: 15
  %tr
    %th= form.label :pronunciation
    %td= form.text_field :pronunciation, size: 15
  %tr
    %th= form.label :phone_number
    %td= form.text_field :phone_number, size: 15
  